@using Microsoft.AspNet.Identity

@if (Request.IsAuthenticated)
{
    @Html.Action("AuthenticatedProfileZone", "Profile")  
}
else
{
    @Html.ActionLink("Sign in", "Login", "Account", new { area = "" }, new { @class = "btn btn-default margin-left", id = "signIn" })
    @Html.ActionLink("Sign up", "Register", "Account", new { area = "" }, new { @class = "btn btn-success margin-left", id = "signUp" })
}
